culo<-function (y)
{
if (is.data.frame(y) == "TRUE" || is.vector(y) == "TRUE") {
c = data.frame(apply(y, 2, mean), apply(y, 2, sigma2),
apply(y, 2, sd), apply(y, 2, max), apply(y, 2, min))
colnames(c) <- c("Mean", "Var", "St.dev", "Max", "Min")
return(c)
}
if (is.data.frame(y) != "TRUE") {
return("FALSE")
}
}
